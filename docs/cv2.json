      "type": "object",
      "properties": {
        "rows": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        },
        "total": {
          "type": "integer",
          "format": "int64"
        }
      },
      "title": "ListArticlesResp",
      "required": [
        "rows",
        "total"
      ]
    },
    "ModuleInfo": {
      "type": "object",
      "properties": {
        "module_id": {
          "type": "integer",
          "format": "int64",
          "description": " 模块ID"
