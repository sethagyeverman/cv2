    "SaveModuleResp": {
      "type": "object",
      "properties": {
        "module_id": {
          "type": "integer",
          "format": "int64",
          "description": " 模块ID"
        },
        "title": {
          "type": "string",
          "description": " 模块标题"
        },
        "score": {
          "type": "number",
          "format": "double",
          "description": " 模块得分"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/map[string]interface"
          },
