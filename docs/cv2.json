        },
        "parameters": [
          {
            "name": "body",
            "description": " 保存模块请求",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/SaveModuleReq"
            }
          }
        ],
        "tags": [
          "resume"
        ]
      }
    },
    "/api/resume/task/{task_id}": {
      "get": {
        "summary": "查询生成任务状态",
        "operationId": "GetTaskStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/TaskStatusResp"
            }
          }
        },
        "parameters": [
          {
            "name": "task_id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "resume"
        ]
      }
    },
    "/api/resume/upload/policy": {
      "post": {
        "summary": "获取简历上传 POST Policy",
        "operationId": "GetUploadPolicy",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/UploadPolicyResp"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "description": " POST Policy 预签名请求",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/UploadPolicyReq"
            }
          }
        ],
        "tags": [
          "resume"
        ]
      }
    },
    "/api/resume/upload/presign": {
      "post": {
        "summary": "获取简历上传预签名 URL",
        "operationId": "GetUploadPresignURL",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/UploadPresignResp"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "description": " 上传预签名请求",
