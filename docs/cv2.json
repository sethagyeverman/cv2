        },
        "title": {
          "type": "string"
        },
        "subtitle": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "thumbnailUrl": {
          "type": "string"
        },
        "status": {
          "type": "integer",
          "format": "int32"
        },
        "viewCount": {
          "type": "integer",
          "format": "int32"
        },
        "publishTime": {
          "type": "string"
        },
        "createTime": {
          "type": "string"
        },
        "updateTime": {
          "type": "string"
        }
      },
      "title": "GetArticleResp"
    },
    "GetResumeReq": {
      "type": "object",
      "title": "GetResumeReq"
    },
    "GetResumeResp": {
      "type": "object",
      "properties": {
        "resume_id": {
          "type": "integer",
          "format": "int64",
          "description": " 简历ID"
        },
        "file_name": {
          "type": "string",
          "description": " 文件名"
        },
        "status": {
          "type": "integer",
          "format": "int32",
          "description": " 状态"
        },
        "total_score": {
          "type": "number",
          "format": "double",
          "description": " 总分"
