    "/api/resume/{resume_id}": {
      "get": {
        "summary": "获取简历详情",
        "operationId": "GetResume",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/GetResumeResp"
            }
          }
        },
        "parameters": [
          {
            "name": "resume_id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "resume"
        ]
      }
    }
