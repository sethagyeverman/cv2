{
  "swagger": "2.0",
  "info": {
    "title": "CV2 API",
    "description": "简历大师 API",
    "version": "v2.0.0"
  },
  "host": "localhost:8888",
  "basePath": "/",
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/api/article/list": {
      "get": {
        "summary": "获取运营文章列表",
        "operationId": "ListArticles",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ListArticlesResp"
            }
          }
        },
        "parameters": [
          {
            "name": "pageNum",
            "in": "query",
            "required": true,
            "type": "integer",
            "format": "int32",
            "default": "1"
          },
          {
            "name": "pageSize",
            "in": "query",
            "required": true,
            "type": "integer",
            "format": "int32",
            "default": "10"
          },
          {
            "name": "title",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "article"
        ],
        "consumes": [
          "multipart/form-data"
        ]
      }
    },
    "/api/article/{articleId}": {
      "get": {
        "summary": "获取运营文章详情",
        "operationId": "GetArticle",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/GetArticleResp"
            }
          }
        },
        "parameters": [
          {
            "name": "articleId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "article"
        ]
      }
    },
    "/api/positions/options": {
      "get": {
        "summary": "Position options",
        "operationId": "Options",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/OptionsResp"
            }
          }
        },
        "tags": [
          "position"
        ]
      }
    },
    "/api/resume/ai-write": {
      "post": {
        "summary": "AI 帮写（SSE 流式返回）",
        "operationId": "AIWrite",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/string"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "description": " AI 帮写请求",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/AIWriteReq"
            }
          }
        ],
        "tags": [
          "resume"
        ]
      }
    },
    "/api/resume/generate": {
      "post": {
        "summary": "生成简历",
        "operationId": "GenerateResume",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/GenerateResumeResp"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "description": " 生成简历请求",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/GenerateResumeReq"
            }
          }
        ],
        "tags": [
          "resume"
        ]
      }
    },
    "/api/resume/module": {
      "post": {
        "summary": "保存简历模块",
