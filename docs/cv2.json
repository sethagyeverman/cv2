      "type": "object",
      "properties": {
        "pageNum": {
          "type": "integer",
          "format": "int32",
          "default": "1"
        },
        "pageSize": {
          "type": "integer",
          "format": "int32",
          "default": "10"
        },
        "title": {
          "type": "string"
        }
      },
      "title": "ListArticlesReq",
      "required": [
        "pageNum",
        "pageSize"
      ]
