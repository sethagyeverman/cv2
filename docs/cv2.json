  "definitions": {
    "AIWriteReq": {
      "type": "object",
      "properties": {
        "resume_id": {
          "type": "integer",
          "format": "int64",
          "description": " 简历ID"
        },
        "module_id": {
          "type": "integer",
          "format": "int64",
          "description": " 模块ID（对应 MongoDB 中的模块项）"
        },
        "info": {
          "type": "string",
          "description": " 额外提示信息"
        }
      },
      "title": "AIWriteReq",
      "required": [
        "resume_id",
        "module_id"
      ]
    },
    "Article": {
      "type": "object",
      "properties": {
        "articleId": {
          "type": "integer",
          "format": "int64"
        },
        "title": {
          "type": "string"
        },
        "subtitle": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "thumbnailUrl": {
          "type": "string"
        },
        "status": {
          "type": "integer",
          "format": "int32"
        },
        "viewCount": {
          "type": "integer",
          "format": "int32"
        },
        "publishTime": {
          "type": "string"
        },
        "createTime": {
          "type": "string"
        },
        "updateTime": {
          "type": "string"
        }
      },
      "title": "Article",
      "required": [
        "articleId",
        "title",
        "status",
        "viewCount"
      ]
